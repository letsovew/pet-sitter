{
  "name": "pet-sitter",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --forceExit",
    "test:silent": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --silent --forceExit",
    "test:coverage": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --coverage --forceExit",
    "test:unit": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest __tests__/unit --forceExit",
    "dev": "cross-env NODE_ENV=dev nodemon --exec babel-node app.js --delay 2 --ignore 'static'"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.25.3",
    "@prisma/client": "^5.18.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "es-dev-server": "^2.1.0",
    "es-module-devserver": "^0.1.6",
    "express": "^4.19.2",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.18.0",
    "nodemon": "^3.1.4",
    "passport": "^0.7.0",
    "prisma": "^5.18.0",
    "prisma-client": "^0.0.0",
    "querystring": "^0.2.1",
    "store": "^2.0.12",
    "url": "^0.11.4"
  },
  "dependencies": {
    "cors": "^2.8.5"
  }
}
